{
  "name": "create-prisma-generator",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "bin": {
    "create-prisma-generator": "dist/bin.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "tsc-watch --onSuccess \"yarn afterBuild\"",
    "afterBuild": "yarn copy-files && yarn publish-locally",
    "publish-locally": "yalc push",
    "prepack": "yarn build",
    "copy-files": "npx ts-node ./scripts/copyFiles.ts src/templates dist/templates"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.13",
    "colors": "^1.4.0",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0"
  },
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/inquirer": "^8.1.3",
    "@types/typescript": "^2.0.0",
    "copyfiles": "^2.4.1",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.5.4"
  },
  "homepage": "https://github.com/YassinEldeeb/create-prisma-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/YassinEldeeb/create-prisma-generator.git"
  },
  "author": "Yassin Eldeeb <yassineldeeb94@gmail.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator",
    "CPG",
    "CRA",
    "boilerplate",
    "setup",
    "development"
  ]
}
